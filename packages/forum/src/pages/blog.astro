---
const pageTitle = "StackUp's Blog";
import BaseLayout from "../layouts/BaseLayout.astro";
const allPosts = await Astro.glob('./posts/*.md');
---
<BaseLayout pageTitle={pageTitle}>
    <h1>Blog</h1>
    <p>News, updates and resources from the StackUp community.</p>
    <ul>
        {allPosts.map((post)=><li><a target="_blank" href={post.url}>{post.frontmatter.title}</a> | {post.frontmatter.pubDate.toString().slice(0,10)}</li>)}
    </ul>
</BaseLayout>
